{
  "name": "puppet-matterbridge",
  "version": "0.1.0",
  "author": "Vox Pupuli",
  "summary": "installs, configures, and manages matterbridge",
  "license": "AGPL-3.0",
  "source": "https://github.com/voxpupuli/puppet-matterbridge.git",
  "project_page": "https://github.com/voxpupuli/puppet-matterbridge",
  "issues_url": "https://github.com/voxpupuli/puppet-matterbridge/issues",
  "description": "This is a nice module to manage matterbridge bridges",
  "tags": [
    "matterbridge",
    "mattermost",
    "irc",
    "slack"
  ],
  "dependencies": [
    {
